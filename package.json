{
  "name": "zenphry-website",
  "private": true,
  "description": "Zenphry Business Restructuring Website",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "prebuild:content": "tsx scripts/prebuild-content.ts",
    "build": "npm run prebuild:content && cross-env NODE_ENV=production react-router build",
    "deploy": "wrangler deploy",
    "dev": "react-router dev",
    "start": "wrangler dev --port 8787",
    "start:dev": "wrangler dev --env dev --port 8787",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "fmt": "prettier --write 'README.md' 'docs/**/*.md'",
    "fmt:check": "prettier --check 'README.md' 'docs/**/*.md'",
    "test": "playwright test",
    "test:smoke": "playwright test tests/smoke.spec.ts --project=chromium --project=mobile-chrome",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "typecheck": "react-router typegen && tsc",
    "typegen": "wrangler types"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-router/cloudflare": "^7.12.0",
    "@react-router/node": "^7.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "isbot": "^5.1.19",
    "lucide-react": "^0.553.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.21.2",
    "@cloudflare/workers-types": "^4.20241224.0",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@react-router/dev": "^7.1.1",
    "@react-router/serve": "^7.1.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^20.17.10",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitest/coverage-v8": "^4.0.9",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.10",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.9",
    "wrangler": "^4.50.0"
  }
}
